<h1>Listing years</h1>

<table>
  <tr>
    <th>Start</th>
    <th>End</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @years.each do |year| %>
  <tr>
    <td><%= l year.start, :format => :long %></td>
    <td><%= l year.end, :format => :long %></td>
    <td><%= link_to 'Edit', edit_year_path(year) %></td>
    <!-- <td><%= link_to 'Destroy', year, confirm: 'Are you sure?', method: :delete %></td> -->
  </tr>
<% end %>
</table>

